{
  "name": "smart-study-group-matcher-system-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\" --ignore-path .gitignore",
    "validate": "npm run lint && npm run format:check",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
